<%= link_to "Create Discount", '/merchant/discounts/new' %>
<% @discounts.each do |discount| %>
  <section id="discount-<%=discount.id %>">
    <ul>
      <%= discount.name %> Discount <br>
      Limit: <%= discount.limit %>
    </ul>
    <%= link_to "Update", "/merchant/discounts/#{discount.id}/edit" %>
    <% end %>
  </section>
